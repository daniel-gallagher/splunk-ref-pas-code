<dashboard script="setup.js">
	<label>Setup</label>
	<row>
		<html>
			<form name="setup_form">
				<input id="_key" type="hidden"></input>
				<div id="divisions" class="input_fields_wrap">
				    <h3>Divisions: </h3>
				</div>
				<button id="divisions_button" class="btn btn-primary btn-small add_field_button">Add Division</button>
				<hr/>
				<div id="locations" class="input_fields_wrap">
				    <h3>Locations:</h3>
				</div>
				<button class="btn btn-primary btn-small add_field_button" id="locations_button">Add Location</button>
				<hr/>
				<div id="policies" class="input_fields_wrap" style="clear:both;">
				    <h3>Policies:</h3>
				</div>
				<button id="policies_button" class="btn btn-primary btn-small add_field_button">Add Policy</button>
				<hr/>
				<input type="button" id="save" class="btn btn-primary" value="Save"></input>

				<div id="saveModal" class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
				  <div class="modal-dialog modal-sm">
				    <div class="modal-content">
				      Settings successfully saved. 
				    </div>
				  </div>
				</div>
			</form>
		</html>
	</row>
</dashboard>